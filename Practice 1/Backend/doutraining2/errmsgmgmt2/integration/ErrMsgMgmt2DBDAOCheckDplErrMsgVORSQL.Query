<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ErrMsgMgmt2DBDAOCheckDplErrMsgVORSQL">
			<desc><![CDATA[check dupicate error message]]></desc>
			<sql><![CDATA[
SELECT  DECODE(SIGN(COUNT(ERR_MSG_CD)),1,'Y','N') DUP_FLG
FROM    COM_ERR_MSG
WHERE   ERR_MSG_CD = @[err_msg_cd]			]]></sql>
			<params>
				<param name="err_msg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
